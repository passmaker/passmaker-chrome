{
  "manifest_version": 2,
  "name": "PassMaker",
  "version": "0.1.0",

  "description": "Password generator",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}
